/* Context menu
---------------*/
.b-context-menu {
    background:url(/web/20121229092325im_/http://worldoftanks.ru/static/3.8.1.3/common/css/block/b-context-menu/img/bg-context-menu.png) 0 0 repeat-y;
    border-bottom: 1px solid #000;
    margin-bottom:20px;
    padding: 0;
    position: relative;
    width:244px;
    z-index: 0;
}

.b-context-menu_wrapper:before {
    -webkit-border-radius: 1px 300px 30px 30px/1px 100px 30px 100px;
    -moz-box-radius: 1px 300px 30px 30px/1px 100px 30px 100px;
    border-radius: 1px 300px 30px 30px/1px 100px 30px 100px;
    /*box-shadow: 0 0 20px rgba(214, 57, 33, 0.7);*/
    -webkit-box-shadow: 0 0 20px rgba(159, 62, 40, 1);
    -moz-box-shadow: 0 0 20px rgba(159, 62, 40, 1);
    box-shadow: 0 0 20px rgba(159, 62, 40, 1);
    content: "";
    height: 33px;
    left: 5px;
    position: absolute;
    right: 60px;
    top: 5px;
    z-index: -1;
}
.b-context-menu p {
	padding:0 9px 3px 0;
	margin: 8px 0;
}
.b-context-menu p:first-child {
	margin:0 0 8px;
}
.b-context-menu_content {
	padding: 0 6px 0 16px;
}
.b-context-menu_wrapper {
	position:relative;
	padding:14px 0 13px 0;
    background:url(/web/20121229092325im_/http://worldoftanks.ru/static/3.8.1.3/common/css/block/b-context-menu/img/bg-context-menu-top.png) 0 0 no-repeat;
}

.b-context-menu_arrow {
	height:24px;
	width:7px;
	background:url(/web/20121229092325im_/http://worldoftanks.ru/static/3.8.1.3/common/css/block/b-context-menu/img/bg-arrow.png) 0 0 no-repeat;
	position:absolute;
	left:-7px;
	top:14px;
}

.b-context-menu_wrapper .intro {
	color:#babfba;
	font-size:11px;
}
.b-context-menu-list {
	line-height:140%;
	text-shadow:1px 1px 2px #010101;
}
.b-context-menu-list li {
	padding:4px 3px 4px 19px;
	margin: 0 2px;
}
.b-context-menu-list .current-page,
.b-context-menu_active-point {
	background: url(/web/20121229092325im_/http://worldoftanks.ru/static/3.8.1.3/common/css/block/b-context-menu/img/active-point.png) repeat-y;
    font-weight: bold;
}
.b-context-menu-list .current-page a {
	cursor: default;
	color: #ffffff;
	background:url(/web/20121229092325im_/http://worldoftanks.ru/static/3.8.1.3/common/css/block/b-context-menu/img/arrow-hover.png) 0 3px no-repeat;
}
.b-context-menu_active-point .b-arrow-link {
	cursor: default;
	color: #ffffff;
	background:url(/web/20121229092325im_/http://worldoftanks.ru/static/3.8.1.3/common/css/block/b-context-menu/img/arrow-hover.png) 0 3px no-repeat;
	padding: 0 0 0 18px;
	display: block;
	word-wrap: break-word;
}
.b-context-menu-list .current-page a:hover {
	text-decoration: none;
}
.b-context-menu-list a {
	color: #cacbcc;
	font-weight: bold;
	text-decoration:none;
	background:url(../img/arrow.png) 0 3px no-repeat;
	padding: 0 0 0 18px;
	display: block;
	word-wrap: break-word;
}
.b-context-menu-list a:hover {
	text-decoration:underline;
	background:url(../img/arrow-hover.png) 0 3px no-repeat;
	color: #ffffff;
}

.b-context-menu-list .disabled,
.b-context-menu-list .disabled:hover {
	color: #cacbcc;
	cursor: auto;
}
.b-context-menu-list .b-dased-link-point:hover {
	text-decoration: none;
}
.b-context-menu-list .b-dased-link-point-decoration {
	color: #cacbcc;/*#cacbcc*/
	border-bottom: 1px dashed #cacbcc;
}
.b-context-menu-list a:hover .b-dased-link-point-decoration {
	color: #ffffff;
	border-bottom: none;
	text-decoration: none;
}
/* END Context menu
--------------------*/

/* Menu tournament
--------------------*/
.b-context-menu__tournament .b-context-menu-list li {
	color:#babfba;
}
.b-context-menu__tournament .b-context-menu-list a {
	color:#bfbfa0;
}
.b-context-menu_tournament .b-context-menu-list span {
	color:#f25322;
}
.b-context-menu__tournament .b-content {
	color:#7e7f80;
	font-size:11px;
}
.b-context-menu__tournament .b-championship-name {
	font-family: "WarHeliosCondCBold", Arial;
	font-weight: bold;
	color: rgb(255, 255, 255);
	text-transform: uppercase;
	margin-bottom: 4px;
	font-size:21px;
}
.b-context-menu__tournament .b-teamname {
	color:#ffdca4;
	font-size:12px;
	font-weight:700;
	line-height:110%;
	padding:2px 0 10px;
}
.b-context-menu__tournament .championship {
	padding:0 0 1px;
}
.b-admin-li .b-admin-linck,
.b-admin-li .b-admin-linck:hover {
    background-image: url(/web/20121229092325im_/http://worldoftanks.ru/static/3.8.1.3/common/css/block/b-context-menu/img/arrow-admin.png) !important;
}
.b-context-menu-img {
	margin: 0 5px 0 16px;
    position: relative;
    display: inline-block;
    *display: inline;
    *zoom:1;
    clear: both;
}
/* END Menu tournament
-----------------------*/

