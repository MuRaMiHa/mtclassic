.b-context-menu__categories {
    background:url(../img/bg-context-menu_categories.png) 0 0 repeat-y;
}
.b-context-menu__categories .b-context-menu_wrapper:before {
    display: none!important;
}
.b-context-menu__categories .b-context-menu_wrapper {
    background:url(../img/bg-context-menu-top_categories.png) 0 0 no-repeat;
	padding:0 0 15px 0;
}
.b-context-menu__categories .b-context-menu_arrow {
	background:url(../img/bg-arrow_categories.png) 0 0 no-repeat;
}

.b-context-menu-list__bottomindent {
	margin-bottom: 8px;
}
