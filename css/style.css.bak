.clr {
  clear: both;
  display: block;
  font-size: 0;
  line-height: 0;
  overflow: hidden; }

.g-line {
  display: inline-block; }
  .g-line:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden; }

/*end layout*/
